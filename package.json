{
  "name": "welp",
  "version": "5.2.0",
  "description": "small and simple immutable flux",
  "main": "lib/Welp.js",
  "jsnext:main": "src/Welp.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/Welp.js dist/welp.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/Welp.js dist/welp.min.js --config webpack.config.production.js",
    "build:examples": "babel-node examples/buildAll.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "examples": "npm install && npm run build && npm start",
    "postversion": "git push && git push --tags",
    "start": "NODE_ENV=DEV node ./webpack.demo.server.js",
    "test": "karma start --single-run",
    "test-dev": "karma start",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rtorr/welp.git"
  },
  "keywords": [
    "immutable",
    "flux",
    "react"
  ],
  "author": "Richard Torruellas <rtorruellas@gmail.com> (http://github.com/rtorr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtorr/welp/issues"
  },
  "homepage": "",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "coveralls": "^2.11.4",
    "css-loader": "^0.18.0",
    "es5-shim": "^4.3.1",
    "eslint": "^1.7.2",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.3.2",
    "expect": "^1.13.0",
    "isparta": "^3.0.3",
    "karma": "^0.13.8",
    "karma-browserstack-launcher": "^0.1.4",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "lcars": "1.1.x",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.3.4",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "npmName": "welp",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "dependencies": {
    "es6-promise": "^3.0.2",
    "eventemitter3": "^1.1.1",
    "events": "^1.1.0",
    "flux": "^2.1.1",
    "immutable": "^3.7.6",
    "isparta-loader": "^1.0.0",
    "object-assign": "^4.0.1",
    "queue": "^3.1.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  }
}
